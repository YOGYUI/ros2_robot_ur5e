<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ur5e">
    <link name="world" />

    <joint name="joint_base" type="fixed">
        <parent link="world" />
        <child link="link_base" />
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>
    <link name="link_base">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 ${pi}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/base.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 ${pi}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/base.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="4.0" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia
                ixx="0.0044333156" ixy="0.0" ixz="0.0"
                iyy="0.0044333156" iyz="0.0"
                izz="0.0072" />
        </inertial>
    </link>

    <joint name="joint_shoulder_pan" type="revolute">
        <parent link="link_base" />
        <child link="link_shoulder" />
        <origin xyz="0 0 0.1625" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${-2*pi}" upper="${2*pi}" effort="150.0" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_shoulder">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 ${pi}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/shoulder.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 ${pi}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/shoulder.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="3.761" />
            <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.00193 -0.02561" />
            <inertia
                ixx="0.0102675" ixy="0.0" ixz="0.0"
                iyy="0.0102675" iyz="0.0"
                izz="0.00666" />
        </inertial>
    </link>

    <joint name="joint_shoulder_lift" type="revolute">
        <parent link="link_shoulder" />
        <child link="link_upper_arm" />
        <origin xyz="0 0 0" rpy="${pi/2} 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${-2*pi}" upper="${2*pi}" effort="150.0" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_upper_arm">
        <visual>
            <origin xyz="0 0 0.138" rpy="${pi/2} 0 ${-pi/2}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/upperarm.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.138" rpy="${pi/2} 0 ${-pi/2}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/upperarm.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="8.058" />
            <origin rpy="0.0 ${pi/2} 0.0" xyz="-0.2125 0.0 0.11336" />
            <inertia
                ixx="0.13388583542" ixy="0.0" ixz="0.0"
                iyy="0.13388583542" iyz="0.0"
                izz="0.0151074" />
        </inertial>
    </link>

    <joint name="joint_elbow" type="revolute">
        <parent link="link_upper_arm" />
        <child link="link_forearm" />
        <origin xyz="-0.425 0 0" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${-pi}" upper="${pi}" effort="150.0" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_forearm">
        <visual>
            <origin xyz="0 0 0.007" rpy="${pi/2} 0 ${-pi/2}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/forearm.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 0.007" rpy="${pi/2} 0 ${-pi/2}" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/forearm.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="2.846" />
            <origin rpy="0.0 ${pi/2} 0.0" xyz="-0.2422 0.0 0.0265" />
            <inertia
                ixx="0.03120936758" ixy="0.0" ixz="0.0"
                iyy="0.03120936758" iyz="0.0"
                izz="0.004095" />
        </inertial>
    </link>

    <joint name="joint_wrist_1" type="revolute">
        <parent link="link_forearm" />
        <child link="link_wrist_1" />
        <origin xyz="-0.3922 0 0.1333" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${-2*pi}" upper="${2*pi}" effort="28.0" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_wrist_1">
        <visual>
            <origin xyz="0 0 -0.127" rpy="${pi/2} 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/wrist1.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.127" rpy="${pi/2} 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/wrist1.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.37" />
            <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.01634 -0.0018" />
            <inertia
                ixx="0.0025599" ixy="0.0" ixz="0.0"
                iyy="0.0025599" iyz="0.0"
                izz="0.0021942" />
        </inertial>
    </link>

    <joint name="joint_wrist_2" type="revolute">
        <parent link="link_wrist_1" />
        <child link="link_wrist_2" />
        <origin xyz="0 -0.0997 -2.044881182297852e-11" rpy="${pi/2} 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${-2*pi}" upper="${2*pi}" effort="${28.0}" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_wrist_2">
        <visual>
            <origin xyz="0 0 -0.0997" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/wrist2.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.0997" rpy="0 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/wrist2.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.3" />
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.01634 -0.0018" />
            <inertia
                ixx="0.0025599" ixy="0.0" ixz="0.0"
                iyy="0.0025599" iyz="0.0"
                izz="0.0021942" />
        </inertial>
    </link>

    <joint name="joint_wrist_3" type="revolute">
        <parent link="link_wrist_2" />
        <child link="link_wrist_3" />
        <origin xyz="0 0.09959999999999999 -2.042830148012698e-11" rpy="${pi/2} ${pi} ${pi}" />
        <axis xyz="0 0 1" />
        <limit lower="${-2*pi}" upper="${2*pi}" effort="28.0" velocity="${pi}" />
        <dynamics damping="0" friction="0" />
    </joint>
    <link name="link_wrist_3">
        <visual>
            <origin xyz="0 0 -0.0989" rpy="${pi/2} 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/visual/wrist3.dae" />
            </geometry>
        </visual>
        <collision>
            <origin xyz="0 0 -0.0989" rpy="${pi/2} 0 0" />
            <geometry>
                <mesh filename="file://$(find ros2_robot_ur5e)/meshes/ur5e/collision/wrist3.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.365" />
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.001159" />
            <inertia
                ixx="9.8904140083e-5" ixy="0.0" ixz="0.0"
                iyy="9.8904140083e-5" iyz="0.0"
                izz="0.0001321171875" />
        </inertial>
    </link>

    <joint name="joint_ft_frame" type="fixed">
        <parent link="link_wrist_3" />
        <child link="link_ft_frame" />
        <origin xyz="0 0 0" rpy="${pi} 0 0" />
    </joint>
    <link name="link_ft_frame" />

    <joint name="joint_flange" type="fixed">
        <parent link="link_wrist_3" />
        <child link="link_flange" />
        <origin xyz="0 0 0" rpy="0 ${-pi/2} ${-pi/2}" />
    </joint>
    <link name="link_flange" />

    <joint name="joint_tool0" type="fixed">
        <parent link="link_flange" />
        <origin xyz="0 0 0" rpy="${pi/2} 0 ${pi/2}" />
        <child link="link_tool0" />
    </joint>
    <link name="link_tool0" />

</robot>